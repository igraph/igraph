                BOOLEAN directed=True, BOOLEAN error=True
        IGNORE: RR, RC, RNamespace

igraph_incident:
        PARAMS: GRAPH graph, OUT VECTOR eids, INTEGER vid, NEIMODE mode=ALL
        IGNORE: RR, RC, RNamespace

