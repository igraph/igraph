> ## Using vertex names
> 
> g <- graph.empty(10)
> V(g)$name <- letters[1:vcount(g)]
> g['a','b'] <- TRUE
> g['b','c'] <- TRUE
> g
IGRAPH DN-- 10 2 -- 
+ attr: name (v/c)
+ edges (vertex names):
[1] a->b b->c
> g[c('a','f'),c('f','a')] <- TRUE
> g
IGRAPH DN-- 10 6 -- 
+ attr: name (v/c)
+ edges (vertex names):
[1] a->b b->c a->f a->a f->f f->a
> g[c('a','c','h'), c('a','b','c'), attr="weight"] <- 3
> print(g, e=T)
IGRAPH DNW- 10 13 -- 
+ attr: name (v/c), weight (e/n)
+ edges and their attributes:
     edge weight
[1]  a->b      3
[2]  b->c     NA
[3]  a->f     NA
[4]  a->a      3
[5]  f->f     NA
[6]  f->a     NA
[7]  a->c      3
[8]  c->a      3
[9]  c->b      3
[10] c->c      3
[11] h->a      3
[12] h->b      3
[13] h->c      3
> 
> ## from-to
> 
> g <- graph.empty(10)
> V(g)$name <- letters[1:vcount(g)]
> g[from=c('a','c','h'), to=c('a','b','c')] <- 1
> g[from=c('a','c','h','d'), to=c('a','b','c','e')]
[1] 1 1 1 0
> g
IGRAPH DN-- 10 3 -- 
+ attr: name (v/c)
+ edges (vertex names):
[1] a->a c->b h->c
> 
> g[from=c('a','c','h','a'), to=c('a','a','a','e'), attr="weight"] <- 3
> g[from=c('a','c','h','a','c','c'), to=c('a','a','a','e','f','b')]
[1]  3  3  3  3  0 NA
> print(g, e=T)
IGRAPH DNW- 10 6 -- 
+ attr: name (v/c), weight (e/n)
+ edges and their attributes:
    edge weight
[1] a->a      3
[2] c->b     NA
[3] h->c     NA
[4] c->a      3
[5] h->a      3
[6] a->e      3
> 
> ## from-to multiple values
> 
> g <- graph.empty(10)
> V(g)$name <- letters[1:vcount(g)]
> g[from=c('a','c','h'), to=c('a','b','c')] <- 1
> g[from=c('a','c','h','a'), to=c('a','a','a','e'), attr="weight"] <- 5:8
> g[from=c('a','c','h','a','c','c'), to=c('a','a','a','e','f','b')]
[1]  5  6  7  8  0 NA
> print(g, e=T)
IGRAPH DNW- 10 6 -- 
+ attr: name (v/c), weight (e/n)
+ edges and their attributes:
    edge weight
[1] a->a      5
[2] c->b     NA
[3] h->c     NA
[4] c->a      6
[5] h->a      7
[6] a->e      8
