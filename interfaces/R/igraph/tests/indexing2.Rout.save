> ## Weighted graph
> 
> igraph.options(print.edge.attributes=TRUE)
> g <- graph.empty(10)
> g <- set.edge.attribute(g, "weight", c(), 1)
> g[1,2] <- 1
> g
IGRAPH D-W- 10 1 -- 
+ attr: weight (e/n)
+ edges and their attributes:
    edge weight
[1] 1->2      1
> g[2,1] <- 2
> g
IGRAPH D-W- 10 2 -- 
+ attr: weight (e/n)
+ edges and their attributes:
    edge weight
[1] 1->2      1
[2] 2->1      2
> g[1,2] <- 3
> g
IGRAPH D-W- 10 2 -- 
+ attr: weight (e/n)
+ edges and their attributes:
    edge weight
[1] 1->2      3
[2] 2->1      2
> g[1:2,2:3] <- -1
> g
IGRAPH D-W- 10 5 -- 
+ attr: weight (e/n)
+ edges and their attributes:
    edge weight
[1] 1->2     -1
[2] 2->1      2
[3] 1->3     -1
[4] 2->2     -1
[5] 2->3     -1
> g[1,2] <- NULL
> g
IGRAPH D-W- 10 4 -- 
+ attr: weight (e/n)
+ edges and their attributes:
    edge weight
[1] 2->1      2
[2] 1->3     -1
[3] 2->2     -1
[4] 2->3     -1
