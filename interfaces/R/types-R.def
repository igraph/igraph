HRG:
    INCONV: if (is.null(%I%)) { \
            %I% <- list(left=c(), right=c(), prob=c(), edges=c(), \
                        vertices=c()) \
            } \
            %I% <- lapply(%I%[c("left","right","prob","edges","vertices")], \
                          as.numeric)
    DEFAULT:
       Default: NULL

RECIP:
    INCONV: %I% <- switch(igraph.match.arg(%I%), 'default'=0, 'ratio'=1)
